<% if @history %>
	<div class="row-fluid">
	<% count = 0 %>
	<% @history.each do |rec| %>
		<% if (count > 0) && (count % 4 == 0) %>
			</div>
			<div class="row-fluid">
		<% end %>
		    <div class="span3 recommended">
				<div class="recommendeditemimage">
		      		<a class="history_outbound_link" href="<%= rec.url %>" target="_blank">
		      			<img class="recommendeditem" src="<%= image_link(rec) %>">
		      		</a>
		      	</div>
		    </div><!--/span-->
		<% count = count + 1 %>
	<% end %>
	</div>
	<%= paginate @history, :remote => true %>
<% end %>
